  "author": "Arne Schoonvliet",
  "description": "Appconfig is een tool die digipolis apps beheert",
  "main": "wwwroot/index.html",
  "scripts": {
    "ngc": "ngc -p ./tsconfig-aot.json",
    "dev": "concurrently \"npm run build\" \"dotnet run\" ",
    "webpack-dev": "set NODE_ENV=development && webpack --progress --colors -w",
    "webpack-prod": "set NODE_ENV=production && webpack --progress --colors",
    "build": "npm run webpack-dev",
    "buildProduction": "npm run ngc && npm run webpack-prod"
  },
